@model List<Catagory>

<div class="container">
    <div class="row pt-4 pb-3">
        <div class="col-6">
            <h1>Catagory List</h1>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="" asp-action="" class="btn btn-primary">Create new catagory</a>
        </div>
    </div>


    <table class="table table-bordered t able-striped">
        <thead> 
            <tr>
                <th>
                    Catagory Name
                </th>
                <th>
                    Display Order
                </th>
            </tr>
        </thead>
        <tbody>

            @foreach (var items in Model)
            {
                <tr>
                    <td>@items.Name</td>
                    <td>@items.DisplayOrder</td>
                </tr>
            }

        </tbody>
    </table>
</div>
